{
    "problemMatcher": [
        {
            "owner": "mypy",
            "pattern": [
                {
                    "regexp": "^(.+):(\\d+):\\s(error|warning):\\s(.+)\\s+\\[(.+)\\]$",
                    "file": 1,
                    "line": 2,
                    "severity": 3,
                    "message": 4,
                    "code": 5
                }
            ]
        }
    ]
}
