{
    "problemMatcher": [
        {
            "owner": "sphinx-spell",
            "severity": "error",
            "pattern": [
                {
                    "regexp": "^(.+):(\\d+):\\s(Spell check):\\s(.+)$",
                    "file": 1,
                    "line": 2,
                    "code": 3,
                    "message": 4
                }
            ]
        }
    ]
}
